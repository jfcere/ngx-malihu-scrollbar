{
  "name": "ngx-malihu-scrollbar",
  "version": "6.0.0-beta.0",
  "description": "Angular scrollbar customization using Malihu jQuery Custom Scrollbar plugin",
  "homepage": "https://github.com/jfcere/ngx-malihu-scrollbar",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@sherweb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcere/ngx-malihu-scrollbar"
  },
  "keywords": [
    "angular",
    "ngx",
    "malihu",
    "scrollbar"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib",
    "postbuild:lib": "cpx ./README.md ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-malihu-scrollbar && linklocal",
    "lint": "yarn lint:lib && yarn lint:demo",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "test": "ng test",
    "coveralls": "cat \"./coverage/lcov.info\" | \"./node_modules/coveralls/bin/coveralls.js\"",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-malihu-scrollbar/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "publish:lib": "npm publish ./dist/lib"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "ngx-malihu-scrollbar": "file:./lib",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "~4.2.1",
    "coveralls": "^2.13.3",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^3.0.0-rc.2",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "tslint-eslint-rules": "^5.2.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "~2.7.2"
  }
}