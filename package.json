{
  "name": "ngx-malihu-scrollbar",
  "version": "10.0.0-beta.0",
  "description": "Angular scrollbar customization using Malihu jQuery Custom Scrollbar plugin",
  "homepage": "https://github.com/jfcere/ngx-malihu-scrollbar",
  "license": "MIT",
  "author": {
    "name": "Jean-Francois Cere",
    "email": "jfcere@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfcere/ngx-malihu-scrollbar"
  },
  "keywords": [
    "angular",
    "ngx",
    "malihu",
    "scrollbar"
  ],
  "scripts": {
    "ng": "ng",
    "start": "yarn link:lib && ng serve",
    "build:demo": "ng build demo",
    "build:lib": "ng build lib --prod",
    "postbuild:lib": "cpx ./README.md ./dist/lib && cpx ./LICENSE ./dist/lib",
    "link:lib": "rimraf node_modules/ngx-malihu-scrollbar && linklocal",
    "lint": "yarn lint:lib && yarn lint:demo",
    "lint:demo": "ng lint demo",
    "lint:lib": "ng lint lib",
    "lint:ci": "ng lint lib --format checkstyle > tslint.xml",
    "test": "ng test",
    "coveralls": "cat \"./coverage/lcov.info\" | \"./node_modules/coveralls/bin/coveralls.js\"",
    "gh-pages:build": "yarn build:demo --prod --aot --base-href /ngx-malihu-scrollbar/",
    "gh-pages:deploy": "angular-cli-ghpages --dir=\"dist/demo\" --name=\"CircleCI\" --email=\"circleci@email.com\" --no-silent",
    "publish:lib": "npm publish ./dist/lib"
  },
  "dependencies": {
    "@angular/animations": "^10.1.1",
    "@angular/common": "^10.1.1",
    "@angular/compiler": "^10.1.1",
    "@angular/core": "^10.1.1",
    "@angular/forms": "^10.1.1",
    "@angular/platform-browser": "^10.1.1",
    "@angular/platform-browser-dynamic": "^10.1.1",
    "@angular/router": "^10.1.1",
    "ngx-malihu-scrollbar": "file:./lib",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.1",
    "@angular-devkit/build-ng-packagr": "~0.1001.1",
    "@angular/cli": "^10.1.1",
    "@angular/compiler-cli": "^10.1.1",
    "@angular/language-service": "^10.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^6.0.0",
    "coveralls": "^2.13.3",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "linklocal": "^2.8.2",
    "ng-packagr": "^10.1.0",
    "rimraf": "^2.7.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
